{
  "extends": "airbnb",
  "rules": {
    "consistent-return": [1],
    "strict": [0],
    "max-len": ["warn", 200],
    "no-underscore-dangle": [1],
    "no-console": "off",
    "react/jsx-props-no-spreading": "off",
    "linebreak-style": 0,
    "no-unneeded-ternary": 0,
    "no-restricted-syntax": 0,
    "no-param-reassign": 0
  },
  "env": {
    "browser": true
  }
}
